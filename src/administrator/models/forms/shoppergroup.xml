<?xml version="1.0" encoding="utf-8"?>
<form>
	<fieldset>

              <field name="id" type="text" default="0" label="JGLOBAL_FIELD_ID_LABEL"
                    readonly="true" class="readonly"
                    description="JGLOBAL_FIELD_ID_DESC" /> 

         <field
                name="state"
                type="list"
                label="JSTATUS"
                description="JFIELD_PUBLISHED_DESC"
                class="inputbox"
                size="1"
                default="1">
                <option value="1">JPUBLISHED</option>
                <option value="0">JUNPUBLISHED</option>
            </field>
            <field
                name="shopper_group_name"
                type="text"
                label="MYMUSE_SHOPPER_GROUP_NAME_LABEL"
                description="MYMUSE_SHOPPER_GROUP_NAME_DESC"
                class="inputbox"
                size="40"
               />
			<field
                name="shopper_group_description"
                type="text"
                label="MYMUSE_SHOPPER_GROUP_DESC_LABEL"
                description="MYMUSE_SHOPPER_GROUP_DESC_DESC"
                class="inputbox"
                size="60"
               />
             <field
                name="discount"
                type="text"
                label="MYMUSE_SHOPPER_GROUP_DISCOUNT_LABEL"
                description="MYMUSE_SHOPPER_GROUP_DISCOUNT_DESC"
                class="inputbox"
                size="6"
               />
                        <field name="checked_out" type="hidden" filter="unset" />
                <field name="checked_out_time" type="hidden" filter="unset" />
	
	</fieldset>

</form>